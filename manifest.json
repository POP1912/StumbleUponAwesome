{
    "name": "SumbleUpon Revived",
    "version": "1.0",
    "description": "StumbleUpon is dead, long live StumbleUpon. An extension for re-discovering the internet in the new decade.",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "permissions": ["storage"],
    "browser_action": {
      "default_icon": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
      }    
    },
    "icons": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    },
    "manifest_version": 2
  }